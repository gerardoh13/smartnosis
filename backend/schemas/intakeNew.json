{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "providerId": {
        "type": "string"
      },
    "firstName": {
      "type": "string",
      "minLength": 2
    },
    "lastName": {
      "type": "string",
      "minLength": 2
    },
    "middleName": {
      "type": "string",
      "minLength": 1
    },
    "dob": {
      "type": "string"
    },
    "sex": {
      "type": "string",
      "minLength": 4,
      "maxLength": 6
    },
    "address1": {
      "type": "string"
    },
    "address2": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string",
      "minLength": 2,
      "maxLength": 2
    },
    "zip": {
      "type": "integer",
      "maxLength": 5
    },
    "insurance": {
      "type": "string",
      "minLength": 2,
      "maxLength": 3
    },
    "phone": {
      "type": "string"
    },
    "phone2": {
      "type": "string"
    },
    "symptoms": {
      "type": "array"
    },
    "conditions": {
      "type": "array"
    },
    "submittedAt": {
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "required": [
    "providerId",
    "firstName",
    "lastName",
    "dob",
    "address1",
    "city",
    "state",
    "zip",
    "insurance",
    "phone",
    "submittedAt"
  ]
}
