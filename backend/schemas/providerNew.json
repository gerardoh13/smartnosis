{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "orgName": {
      "type": "string",
      "minLength": 2,
      "maxLength": 30
    },
    "npi": {
      "type": "string",
      "minLength": 10,
      "maxLength": 10
    },
    "email": {
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "format": "email"
    },
    "phone": {
      "type": "string"
    },
    "address1": {
      "type": "string"
    },
    "address2": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string",
      "minLength": 2,
      "maxLength": 2
    },
    "zip": {
      "type": "integer",
      "maxLength": 5
    },
    "password": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20
    }
  },
  "additionalProperties": false,
  "required": [
    "orgName",
    "npi",
    "email",
    "phone",
    "address1",
    "city",
    "state",
    "zip",
    "password"
  ]
}
